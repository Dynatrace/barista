{
  "proseWrap": "always",
  "endOfLine": "lf",
  "printWidth": 80,
  "singleQuote": true,
  "trailingComma": "es5",
  "htmlWhitespaceSensitivity": "ignore",
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    },
    {
      "files": "*.html",
      "options": { "htmlWhitespaceSensitivity": "css", "parser": "vue" }
    },
    {
      "files": "*.spec.ts",
      "options": { "htmlWhitespaceSensitivity": "css", "parser": "typescript" }
    }
  ]
}
